{
  "expo": {
    "name": "Pelycan",
    "slug": "Pelycan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logopelycan.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logopelycan.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pelycan.app",
      "versionCode": 1,
      "networkSecurityConfig": {
        "cleartextTrafficPermitted": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logopelycan.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logopelycan.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0fac2a8c-fc0d-4f98-86ff-76fdbc4e21f1"
      }
    }
  }
}
